{
  "name": "mez-web",
  "version": "1.0.0",
  "description": "Zanaremo's web app.",
  "main": "index.ts",
  "scripts": {
    "start": "npx ts-node index.ts",
    "watch": "nodemon --watch '**/*.ts' --exec 'ts-node' index.ts",
    "build": "npx tsc",
    "local-build-all": "npx tsc && cd frontend && npm run build",
    "heroku-postbuild": "cd frontend && npm install && npm run build && cd .."
  },
  "author": "Mez Rashid",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.6",
    "express": "^4.17.1",
    "npx": "^10.2.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2"
  }
}
